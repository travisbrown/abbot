    att.global.facs.attribute.facs,
    att.declaring.attribute.decls,
    att.typed.attribute.type,
    att.typed.attribute.subtype,
    
    ## supplies the sort key for this term in an index.
    attribute sortKey {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
